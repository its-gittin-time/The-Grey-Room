[internationalization]

rendering/root_node_layout_direction=1
rendering/root_node_auto_translate=false

[layer_names]

3d_physics/layer_1="Building"
3d_physics/layer_2="Player"
3d_physics/layer_3="Interactable"
3d_physics/layer_4="Wire"
3d_physics/layer_5="Lock"

[network]

limits/debugger/max_queued_messages=4096

[rendering]

textures/vram_compression/import_etc2_astc=true
lights_and_shadows/directional_shadow/size=2048
reflections/sky_reflections/roughness_layers=6
global_illumination/gi/use_half_resolution=true
camera/depth_of_field/depth_of_field_bokeh_shape=0
environment/ssao/quality=1
environment/glow/upscale_mode=0
environment/volumetric_fog/volume_size=16
environment/volumetric_fog/volume_depth=32
environment/volumetric_fog/use_filter=0
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
lightmapping/denoising/denoiser=1
anti_aliasing/quality/msaa_3d=1
anti_aliasing/quality/screen_space_aa=1
anti_aliasing/quality/use_debanding=true

[shader_globals]

colour_matrix={
"type": "mat3",
"value": Basis(1, 0, 0, 0, 1, 0, 0, 0, 1)
}
outline_colour={
"type": "vec4",
"value": Vector4(1, 1, 1, 1)
}
blue_noise={
"type": "sampler2D",
"value": "res://textures/noise/blue_noise.png"
}
