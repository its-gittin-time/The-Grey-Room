;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="The Grey Room"
run/main_scene="res://scenes/tutorial.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://textures/icon.svg"

[autoload]

Global="*res://scripts/global.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[editor]

naming/node_name_casing=2
naming/script_name_casing=2

[file_customization]

folder_colors={
"res://audio/": "purple",
"res://materials/": "orange",
"res://models/": "blue",
"res://resources/": "pink",
"res://scenes/": "green",
"res://scripts/": "yellow"
}

[filesystem]

import/fbx2gltf/enabled=false
import/blender/enabled=false

[global_group]

grabbable=""
interactable=""
box=""

[gui]

theme/custom="res://resources/global_theme.tres"

[importer_defaults]

scene={
"meshes/generate_lods": false
}
3d_texture={
"mipmaps/limit": 3
}
2d_array_texture={
"mipmaps/limit": 3
}
texture={
"detect_3d/compress_to": 2,
"process/size_limit": 4096,
"roughness/mode": 1
}

