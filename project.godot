; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="The Grey Room"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://textures/icon.svg"

[audio]

buses/default_bus_layout="res://resources/default_bus_layout.tres"
driver/mix_rate.web=44100

[autoload]

Global="*res://scripts/global.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/assert_always_false=2
gdscript/warnings/integer_division=2
gdscript/warnings/narrowing_conversion=2

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"

[editor]

naming/node_name_casing=2
naming/script_name_casing=2
export/convert_text_resources_to_binary=false

[file_customization]

folder_colors={
"res://audio/": "purple",
"res://materials/": "orange",
"res://models/": "blue",
"res://resources/": "pink",
"res://scenes/": "green",
"res://scripts/": "yellow"
}

[filesystem]

import/fbx2gltf/enabled=false
import/blender/enabled=false

[global_group]

grabbable=""
interactable=""
box=""
wire=""

[gui]

theme/custom="res://resources/global_theme.tres"
theme/custom_font="res://resources/fonts/IBMPlexSans-Medium.ttf"

[importer_defaults]

scene={
"gltf/embedded_image_handling": 0,
"meshes/generate_lods": false,
"meshes/light_baking": 2
}
3d_texture={
"mipmaps/limit": 3
}
2d_array_texture={
"mipmaps/limit": 3
}
texture={
"detect_3d/compress_to": 2,
"process/size_limit": 4096,
"roughness/mode": 1
}

